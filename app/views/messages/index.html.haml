.hedder チャットグループが作成されました。
.contents
  .contents__side-content
    .contents__side-content__user
      %h5 seo
      .contents__side-content__user--pencil
      .contents__side-content__user--gear
    .contents__side-content__group
      .contents__side-content__chat-group
        %p.contents__side-content__chat-group__name sample
        %p.contents__side-content__chat-group__message Good by world!


  .contents__main-content
    .contents__main-content__group.clearfix
      .contents__main-content__group__left
        %h2 sample
        %p Memcers: seo neko
      .contents__main-content__group__right
        %a{ type: "submit"}Edit

    .contents__main-content__chat
      %ul.contents__main-content__chat__messages
        %li.contents__main-content__chat__messages__message
          .contents__main-content__chat__messages__message__message-hedder.clearfix
            %p.contents__main-content__chat__messages__message__message-hedder__name sample
            %p.contents__main-content__chat__messages__message__message-hedder__time 2016/09/21 06:16:55
          %p.contents__main-content__chat__messages__message__message-body Hellow world!
        %li.contents__main-content__chat__messages__message
          .contents__main-content__chat__messages__message__message-hedder.clearfix
            %p.contents__main-content__chat__messages__message__message-hedder__name neko
            %p.contents__main-content__chat__messages__message__message-hedder__time 2016/09/21 06:16:55
          %p.contents__main-content__chat__messages__message__message-body Good by world!
    .contents__main-content__footer
      .contents__main-content__footer__form
        %textarea.contents__main-content__footer__form__body{ type: 'text', id: "message", placeholder: "type a message" }
      %input.contents__main-content__footer__btn{ type: "submit", name:"commit", value: "Send" }
